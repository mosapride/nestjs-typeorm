{
  "name": "nestjs-typeorm",
  "version": "0.0.1",
  "description": "",
  "author": "mosapride",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "nest": "nest",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env LEVEL=debug nest start",
    "start:dev": "rimraf dist && cross-env LEVEL=debug nest start --watch",
    "start:debug": "cross-env LEVEL=debug nest start --debug 9229 --watch",
    "start:test": "cross-env LEVEL=test nest start --debug 9229 --watch",
    "start:release": "nest build && cross-env LEVEL=release node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:debug": "cross-env LEVEL=debug node --require ts-node/register ./node_modules/typeorm/cli.js -f src/util/typeorm-config.ts",
    "typeorm:test": "cross-env LEVEL=test node --require ts-node/register ./node_modules/typeorm/cli.js -f src/util/typeorm-config.ts",
    "typeorm:release": "cross-env LEVEL=release node --require ts-node/register ./node_modules/typeorm/cli.js -f src/util/typeorm-config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.1.4",
    "bcrypt": "^5.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.7",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-cookie": "^1.0.8",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "source-map-support": "^0.5.19",
    "typeorm": "^0.2.28",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
